@page
@model TwitchBot.Pages.Config.MessagesModel
@{
    ViewData["Title"] = "Configurations - Messages";
}
@{
    if (Model.SuccessfulSave)
    {
        <div class="alert alert-success" role="alert">
            Changes Saved Successfully!
        </div>
